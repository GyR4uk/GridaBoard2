    "zlib": "^1.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "https": "set HTTPS=true&&react-scripts start",
    "build": "set GENERATE_SOURCEMAP=true && react-scripts build",
    "test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "e:start": "concurrently \"cross-env NODE_ENV=development BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "e:build": "npm run build && electron-builder",
    "deploy": "npm run  build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-redux": "7.1.14",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "eslint": "7.15.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-react": "7.21.5",
    "typescript": "4.1.2",
    "wait-on": "^5.2.1"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-prototype-builtins": "off",
    " @typescript-eslint/no-inferrable-types": "off"
  },
  "description": "GridaBoard by Neo smartpen",
  "author": "NeoLAB Convergence",
  "build": {
    "asar": true,
    "appId": "com.neosmartpen.gridaboard",
    "productName": "GridaBoard2",
    "protocols": {
      "name": "gridaboard",
      "schemes": [
        "gridaboard"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "createDesktopShortcut": true
    }
  },
  "main": "public/electron.js",
  "homepage": "./"
}
